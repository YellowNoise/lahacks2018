{
	"name": "Watcher",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Chrome extension description written here.",
	"icons": {
		"19": "images/eye.png",
		"38": "images/eye.png",
		"128": "images/eye.png"
	},
	"browser_action": {
		"default_title": "browser_action: default_title",
		"default_icon": {
			"19": "images/eye.png",
			"38": "images/eye.png"
		},
		"default_popup": "html/main.html"
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": ["js/jquery.min.js", "js/background.js"]
		}
	],
	"background": {
		"scripts": ["js/background.js"]
	},
	"options_page": "html/options.html",
	"homepage_url": "http://google.com/"
}